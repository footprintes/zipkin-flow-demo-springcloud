server:   
      port: 8761  
eureka:  
       instance:   
           hostname: 127.0.0.1  
       client:   
            registerWithEureka: false  
            fetchRegistry: false  
            serviceUrl:   
                     defaultZone: http://${eureka.instance.hostname}:${server.port}/eureka/
spring:
  application: 
    name: springCloudEureka
  zipkin: 
    base-url: http://localhost:9411/